# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
name = JimSchofield
email = oldcoyote@gmail.com

[alias]
fb = "!git checkout $(git branch | fzf)"
ll = "log --oneline"

[column]
ui = auto

[branch]
sort = -committerdate

[tag]
sort = version:refname

[init]
defaultBranch = main

[diff]
algorithm = histogram
colorMoved = plain
mnemonicPrefix = true
renames = true

[push]
default = simple
autoSetupRemote = true
followTags = true

[fetch]
prune = true
pruneTags = true
all = true

[rerere]
enabled = true
autoupdate = true
[help]
autocorrect = prompt
[pull]
rebase = true
